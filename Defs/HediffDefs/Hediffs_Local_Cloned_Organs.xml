<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <HediffDef ParentName="DiseaseBase">
        <defName>ClonedHeartImplant</defName>
        <label>cloned heart graft</label>
        <labelNoun>a cloned heart along side a normal heart</labelNoun>
        <tendable>true</tendable>
        <comps>
            <li Class="HediffCompProperties_Immunizable">
                <severityPerDayNotImmune>0.025</severityPerDayNotImmune>
            </li>
            <li Class="HediffCompProperties_TendDuration">
                <baseTendDurationHours>24</baseTendDurationHours>
                <severityPerDayTended>0.05</severityPerDayTended>
            </li>
        </comps>
        <stages>
            <li>
                <label>initial</label>
                <partEfficiencyOffset>-0.05</partEfficiencyOffset>
            </li>
            <li>
                <minSeverity>0.2</minSeverity>
                <label>early</label>
                <partEfficiencyOffset>-0.1</partEfficiencyOffset>
                <painOffset>0.05</painOffset>
                <hediffGivers>
                    <li Class="HediffGiver_Random">
                        <hediff>HeartAttack</hediff>
                        <mtbDays>1200</mtbDays>
                        <partsToAffect>
                            <li>Heart</li>
                        </partsToAffect>
                    </li>
                </hediffGivers>
            </li>
            <li>
                <minSeverity>0.4</minSeverity>
                <label>pre-mature</label>
                <partEfficiencyOffset>-0.15</partEfficiencyOffset>
                <painOffset>0.1</painOffset>
                <hediffGivers>
                    <li Class="HediffGiver_Random">
                        <hediff>HeartAttack</hediff>
                        <mtbDays>100</mtbDays>
                        <partsToAffect>
                            <li>Heart</li>
                        </partsToAffect>
                    </li>
                </hediffGivers>
            </li>
            <li>
                <minSeverity>0.6</minSeverity>
                <label>near-mature</label>
                <partEfficiencyOffset>-0.35</partEfficiencyOffset>
                <painOffset>0.2</painOffset>
                <hediffGivers>
                    <li Class="HediffGiver_Random">
                        <hediff>HeartAttack</hediff>
                        <mtbDays>60</mtbDays>
                        <partsToAffect>
                            <li>Heart</li>
                        </partsToAffect>
                    </li>
                </hediffGivers>
            </li>
            <li>
                <minSeverity>0.9</minSeverity>
                <label>mature</label>
                <lifeThreatening>true</lifeThreatening>
                <partEfficiencyOffset>-0.6</partEfficiencyOffset>
                <hediffGivers>
                    <li Class="HediffGiver_Random">
                        <hediff>HeartAttack</hediff>
                        <mtbDays>30</mtbDays>
                        <partsToAffect>
                            <li>Heart</li>
                        </partsToAffect>
                    </li>
                </hediffGivers>
            </li>
        </stages>
    </HediffDef>

