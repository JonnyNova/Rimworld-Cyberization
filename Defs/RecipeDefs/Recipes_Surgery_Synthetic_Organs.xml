<?xml version="1.0" encoding="utf-8" ?>
<Defs>
        <RecipeDef ParentName="SurgeryFlesh">
        <defName>InstallSyntheticLung</defName>
        <label>install synthetic lung</label>
        <description>Install a synthetic lung.</description>
        <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
        <jobString>Installing synthetic lung.</jobString>
        <workAmount>2500</workAmount>
        <skillRequirements>
        <Medicine>5</Medicine>
        </skillRequirements>
        <ingredients>
        <li>
            <filter>
                <categories>
                    <li>Medicine</li>
                </categories>
            </filter>
            <count>2</count>
        </li>
        <li>
            <filter>
                <thingDefs>
                    <li>SyntheticLung</li>
                </thingDefs>
            </filter>
            <count>1</count>
        </li>
        </ingredients>
        <fixedIngredientFilter>
        <categories>
            <li>Medicine</li>
        </categories>
        <thingDefs>
            <li>SyntheticLung</li>
        </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>Lung</li>
        </appliedOnFixedBodyParts>
        <addsHediff>SyntheticLung</addsHediff>
    </RecipeDef>

